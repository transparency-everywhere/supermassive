
*{margin:0px; padding:0px;}
.clearfix {
  *zoom: 1;
}
.clearfix:before,
.clearfix:after {
  display: table;
  content: "";
  line-height: 0;
}
.clearfix:after {
  clear: both;
}

p,ul,li,span,h1,h2,h3,h4,h5,h6{margin:0px; padding:0px; font-family: 'Roboto Condensed', sans-serif;}

img{max-width:100%; border:none;}

a{outline:none; text-decoration:none;}
a:hover{text-decoration:none;}

ol,ul{list-style:none;}

body{font-family: 'Roboto Condensed', sans-serif;}

h1, h2, h3, h4, h5, h6{font-family: 'Roboto Condensed', sans-serif;}

.clr{clear:both;}

#header{margin:0px; padding:0px; background:url(../images/header.jpg) repeat-x;} 

.social{ margin:0px; padding:0px; width:100%; background:url(../images/top-social.jpg) repeat-x;}

.social ul{ float:right; list-style-type:none;}

.social ul li{margin:0px; padding:0px; float:left;}

.social ul li:hover{ opacity:0.5;}

.call_us{ margin:18px 0px 18px 0px; padding:0px; width:50%; float:right;}

.call_us ul{ margin:0px; padding:0px; float:right;}

.call_us ul li{ float:left; list-style-type:none; margin:0px; padding:0px 15px 0 8px;  font-weight:400; color:#878787;  background:url(../images/divider.png) no-repeat right; font-size:14px; line-height:13px; }

.call_us ul li.last_em{ background:none; margin:0px; font-weight:400; padding:0px 0 0 8px;}

.call_us ul li img{ float:left; margin:0 5px 0 0;}



#nav{ margin:0px 0 0 0; padding:0px; width:100%; float:right;}

#nav ul { margin:0px; padding:0px;}

#nav ul li{ margin:2px 0 0 0; padding:0px 20px 0 12px; background:url(../images/divider.png) no-repeat right; float:left; position:relative;}

#nav ul li a{ margin:0px 0 0 0; padding:0px; color:#3b3b3b; text-decoration:none; font-size:14px; line-height:30px; font-weight:400;}

#nav ul li.last{ background:none; padding:0px 0px 0 10px; margin:0px;}

#nav ul li a.active{  border-bottom:5px solid #038cfc; }


#nav ul li a:hover{ border-bottom:5px solid #038cfc; padding:0px 0 14px 0;}

#nav ul li a.home{ background:url(../images/home.jpg) no-repeat; padding:5px 10px 0px 10px;}

#nav ul ul{ display:none;}

#nav ul li:hover ul{ position:absolute; display:block; top:42px; left:0px; width:170px; box-shadow: 1px 2px 3px rgba(0,0,0,0.2); z-index:99999999; background:#fff; border-bottom:1px solid #f1f1f1; border-left:1px solid #f1f1f1;
border-right:1px solid #f1f1f1; border-radius:0 0 5px 5px;}

#nav ul li:hover ul li{ background:none; width:100%}
#nav ul li:hover ul li a{ border:none; color:#333;  border-bottom:1px dashed #CCCCCC; background:url(../images/menu-arrow.png) no-repeat 0 6px; padding:0 0 0 10px;}
#nav ul li:hover ul li a:hover{ color:#54b0f1; background:url(../images/menu-arrow.png) no-repeat 0 -24px;}

#mainContainer{ margin:0px; padding:0px;}

#slide{ margin:0px; padding:0px;}

#slide img{ width:100%; min-height:494px; margin:0px; padding:0px;}

.news{ margin:0px; padding:0px; width:100%; background:#f3f3f3;  border-bottom:2px solid #dedede;}

.news h4{ margin:0px; padding:0px 38px 0 10px; color:#FFF; float:left; background:url(../images/news_heading.jpg) no-repeat 0px; font-size:17px; line-height:42px;}

.news P{ color:#595959; font-weight:300; line-height:42px; font-size:14px; margin:0px; padding:0px;}

ul.news_ar{ float:right; line-height:42px; margin:0px 0 0 0;}

ul.news_ar li{ margin:0px; padding:0px; float:left;}

#box{ margin:30px 0 30px 0; padding:0px;}

#box ul { margin:0px; padding:0px;}

#box ul li{  position:relative;}

#box ul li img{ width:100%;}

#box ul li h4{ position:absolute; bottom:0px; margin:0px; right:15px; background:rgba(236,201,47,0.8); font-weight:400; font-size:20px; padding:16px 0 16px 0; text-align:center; text-transform:uppercase; color:#fff; width:89.4%; border-radius:0 0 3px 3px; min-height:76px;}


#box ul li h4.box1{  background:rgba(23,158,66,0.8);  padding:26px 0 26px 0;}

#box ul li h4.box2{  background:rgba(225,132,31,0.8);  padding:26px 0 26px 0;}

#box ul li h4.box3{  background:rgba(36,155,252,0.8);  padding:26px 0 26px 0;}

#box ul li.lst_box{ margin:0px; padding:0px;}

#footer{ margin:0px; padding:35px 0 0px 0; background:url(../images/foot_bg.jpg) repeat-x  #141311;}

#footer h5{ margin:0px 0 12px 0; padding:0px; font-size:15px; color:#FFF; text-transform:uppercase; font-weight:400;}

#footer h5.bot{ margin:24px 0 12px 0;}

#footer ul {margin:0px; padding:0px;}

#footer ul li{}

#footer ul li ul li{ margin:0px; background:url(../images/foot_ar.png) no-repeat 0px; float:none; width:100%;}

#footer ul li ul li a{ margin:0px;  padding:0px 0 0 7px; color:#696969; font-size:13px; line-height:24px; border-bottom:1px solid #242323;}

#footer ul li ul li a:hover{ text-decoration:none; color:#e7e7e7;}

#footer ul li p{ font-size:13px; color:#696969;}

#footer ul li p a{ text-decoration:underline; color:#696969;}

#footer ul li p.text{ font-size:13px; color:#c8c8c8; text-transform:uppercase;}

.copyright{ margin:25px 0 0 0; padding:0px; background:#151412; border-top:1px solid #262524;}

.copyright p{ font-size:13px; line-height:45px; color:#696969;}

.copyright p span{ color:#FFF; text-transform:uppercase;}

.copyright p.privacy{ float:right;}

.copyright p.privacy a{ color:#696969;}

.bdr{ border-right:1px dashed #dedede;}
.maincontent p{ font-size:12px; line-height:20px;}
.maincontent img{ float:right;}
.maincontent h5{ font-size:20px; line-height:30px; color:#000; font-family: 'Roboto Condensed', sans-serif;}
.maincontent ul{ width:100%;}
.maincontent ul li{ float:left; margin:0 5px 0 0;}
.maincontent ul li img{ width:16px; height:16px;}
.maincontent ul li img:hover{ opacity:0.8;}

.cat h4{font-family: 'Roboto Condensed', sans-serif; font-size:18px; line-height:35px; padding:0 0 0 10px; background:#54b0f1; font-weight:400; color:#fff; border-bottom:1px solid #000;}
.cat ul li{border-bottom:1px solid #CCC; }
.cat ul li a{ font-size:13px; line-height:30px; background:url(../images/cat_ar.png) no-repeat 0px; padding:0 0 0 15px; font-family: 'Roboto Condensed', sans-serif;}



.slicknav_menu {
	display:none;
}

@media screen and (max-width: 40em) {
	.js #menu {
		display:none;
	}
	
	.js .slicknav_menu {
		display:block;
	}
}


.social-icons ul {
    list-style: none;
    margin: -3px 0px;
    float: right;
}
.social-icons ul li {
	display: block;
	float: left;
	margin: 0;
	padding: 0;
}
.social-icons ul li a {
	-webkit-transition: all 0.2s ease 0s;
	-moz-transition: all 0.2s ease 0s;
	-o-transition: all 0.2s ease 0s;
	transition: all 0.2s ease 0s;
	display: block;
	width: 30px;
	height: 30px;
	text-indent: -9999px;
	background-position: 0px 0px;
	background-repeat: no-repeat;
	opacity: 0.8;
}


.social-twitter a { background:url(../images/twitter.png) no-repeat 0 0;}
.social-twitter a:hover { background:url(../images/twitter.png)#48c4d2 no-repeat 0px -30px;  }

.social-facebook a{ background:url(../images/facebook.png) no-repeat 0 0; }
.social-facebook a:hover{ background:url(../images/facebook.png)#3b5998 no-repeat 0px -30px;}

.social-google a{background:url(../images/google.png) no-repeat 0 0;}
.social-google a:hover{background:url(../images/google.png)#dd4b39 no-repeat 0 -30px; }

.social-linkedin a{background:url(../images/linkedin.png) no-repeat 0 0;}
.social-linkedin a:hover{background:url(../images/linkedin.png)#71b2d0 no-repeat 0 -30px;}

.social-youtube a{background:url(../images/youtube.png) no-repeat 0 0;}
.social-youtube a:hover{background:url(../images/youtube.png)#f45750 no-repeat 0 -30px;}



